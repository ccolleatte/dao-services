# Paseo Testnet Configuration
# Copy this file to .env and fill in your values
# NEVER commit .env to git

# RPC URL for Paseo Testnet
RPC_URL="https://paseo-rpc.polkadot.io"

# Deployer private key (KEEP SECRET!)
# Generate with: cast wallet new
# Or export from MetaMask: Settings > Security & Privacy > Reveal Private Key
PRIVATE_KEY="0x..."

# Admin address (deployer address)
# Get with: cast wallet address --private-key $PRIVATE_KEY
ADMIN_ADDRESS="0x..."

# Block explorer API key (optional, not needed for Paseo)
ETHERSCAN_API_KEY=""

# Gas settings (optional)
# Leave empty to use defaults
GAS_PRICE=""
GAS_LIMIT=""

# Deployment configuration (optional)
# Initial treasury funding (in wei)
INITIAL_TREASURY_FUNDING="0"

# Initial members to add (comma-separated addresses)
# Format: address:rank,address:rank
# Example: 0x123...:1,0x456...:2
INITIAL_MEMBERS=""
